{
  "name": "@alita/alita-layout",
  "version": "3.0.5",
  "description": "The generic h5 layout in umi uses antd-mobile. alita@3 or umi@4",
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/alitajs/alita-layout#readme",
  "scripts": {
    "build": "father-build",
    "prepublishOnly": "father-build",
    "prettier": "node ./scripts/prettier.js",
    "publish": "npm publish --access public --next"
  },
  "dependencies": {
    "antd-mobile-v2": "2.3.4",
    "react": "^16.8.6",
    "react-router-dom": "^6.1.1"
  },
  "devDependencies": {
    "@types/react-dom": "^16.8.4",
    "babel-plugin-import": "^1.12.0",
    "father-build": "^1.17.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
